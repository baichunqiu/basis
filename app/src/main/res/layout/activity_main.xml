<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:id="@+id/load"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimens_60px"
        android:layout_marginRight="@dimen/dimens_60px"
        android:text="加载等待框展示" />

    <Button
        android:id="@+id/widgetActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimens_60px"
        android:layout_marginRight="@dimen/dimens_60px"
        android:text="指示器展示" />

    <Button
        android:id="@+id/recycle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimens_60px"
        android:layout_marginRight="@dimen/dimens_60px"
        android:text="Recycle" />

    <Button
        android:id="@+id/listui"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimens_60px"
        android:layout_marginRight="@dimen/dimens_60px"
        android:text="列表界面展示" />

    <Button
        android:id="@+id/wrap_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimens_60px"
        android:layout_marginRight="@dimen/dimens_60px"
        android:text="WrapActionBar" />
    <Button
        android:id="@+id/preview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimens_60px"
        android:layout_marginRight="@dimen/dimens_60px"
        android:text="图片预览" />
    <Button
        android:id="@+id/net_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimens_60px"
        android:layout_marginRight="@dimen/dimens_60px"
        android:text="网络接口封装调试" />
</LinearLayout>