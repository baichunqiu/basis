<?xml version="1.0" encoding="utf-8"?>
<com.basis.percent.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.basis.widget.TitleBar
        android:id="@+id/bsi_v_titleBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_hind="true"
        app:title="UI" />

    <Button
        android:id="@+id/listActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimens_60px"
        android:layout_marginRight="@dimen/dimens_60px"
        android:text="list activity" />

    <Button
        android:id="@+id/widgetActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimens_60px"
        android:layout_marginRight="@dimen/dimens_60px"
        android:text="widget activity" />

    <Button
        android:id="@+id/camera"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimens_60px"
        android:layout_marginRight="@dimen/dimens_60px"
        android:text="camera activity" />
</com.basis.percent.PercentLinearLayout>